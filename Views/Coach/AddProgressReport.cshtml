@model List<CourseProject_SoftwareArchitecture.Models.Enrollment>
<h2>Enter Progress Report </h2>
<h3>Course: @Model[0].Session.SessionId</h3>
<form asp-controller="Coach" asp-action="AddProgressReport" method="post">
    <table border="1">
        <tr>
            <th></th>
            <th>Swimmer ID</th>
            <th>Report</th>
        </tr>
        @for (int i = 0; i < Model.Count(); i++)
            {
                <tr>
                    <td><input asp-for="@Model[i].EnrollmentId"
                               type="hidden"
                               value="@Model[i].EnrollmentId" />
                               </td>
                    <td>@Model[i].SwimmerId</td>
                    <td><input asp-for="@Model[i].ProgressReport" />
                    </td>
                </tr>
            }
    </table>
   <input type="submit" value="Submit" />
          </form>